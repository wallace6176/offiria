	<creationDate>14/11/11</creationDate>
	<author>JomSocial Team</author>
	<authorEmail>support@jomsocial.com</authorEmail>
	<authorUrl>http://www.jomsocial.com</authorUrl>
	<copyright>Copyright (C) Slashes n Dots Sdn Bhd. All rights reserved.</copyright>
	<license>GPL</license>
	<version>1.7.2</version>
	<description>Enterprise 2.0</description>
	<files>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<folder>images</folder>
		<folder>css</folder>
		<folder>js</folder>
	</files>
	<positions>
		<position>left</position>
		<position>right</position>
		<position>login</position>
		<position>breadcrumbs</position>
	</positions>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="colorVariation"
					type="list"
					default="white"
					label="Color Variation"
					description="Base Color of template">
					<option
						value="blue">blue</option>
					<option
						value="red">red</option>
				</field>
			</fieldset>
		</fields>
	</config>
</install>
